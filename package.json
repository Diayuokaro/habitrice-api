{
  "name": "habitrice-api",
  "version": "1.0.0",
  "private": true,
  "author": "Denis Skvortsov <diayuokaro@gmail.com>",
  "main": "index",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config index",
    "dev": "nodemon -r dotenv/config index"
  },
  "imports": {
    "#*": {
      "default": "./*"
    },
    "#config": {
      "default": "./config/index.js"
    },
    "#controllers/*": {
      "default": "./controllers/*"
    },
    "#core": {
      "default": "./core/init.js"
    },
    "#database": {
      "default": "./database/index.js"
    },
    "#middleware": {
      "default": "./middleware/index.js"
    },
    "#models/*": {
      "default": "./models/*"
    },
    "#modules/*": {
      "default": "./models/*"
    },
    "#routes": {
      "default": "./routes/index.js"
    },
    "#utils/*": {
      "default": "./models/*"
    }
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^8.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
